        .modal_content .modal-body,
        .modal_content .modal-header {
            background: #242832;
            z-index: 22;
            border: none;
        }

        .modal_content:before {
            content: "";
            height: 100%;
            width: 100%;
            z-index: 0;
            position: absolute;
            top: 5px;
            left: -5px;
            border-radius: 10px;
            background: linear-gradient(45deg, red, blue, yellow);
        }

        .modal-body {
            max-height: 80vh;
            overflow-y: scroll;
            -ms-overflow-style: none;
            /* for Internet Explorer, Edge */
            scrollbar-width: none;
            /* for Firefox */
        }


        .modal-body::-webkit-scrollbar {
            display: none;
            /* for Chrome, Safari, and Opera */
        }

        .navbar_styles {
