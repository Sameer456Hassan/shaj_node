    <script>


        $(document).ready(function () {

            AOS.init()


            $('.close').click(function () {
                $('.modal').removeClass('show')
                $('.modal').css('display', 'none')
            })

            $(document).click(function (e) {
                if ($(".modal").hasClass("show") == true) {
                    if (e.target != $('.modal')) {
                        $('.modal').removeClass('show')
                        $('.modal').css('display', 'none')
                    }
                }
            });
        });
    </script>
